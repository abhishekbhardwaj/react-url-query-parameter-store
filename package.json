{
  "name": "react-url-query-parameter-store",
  "version": "0.0.0",
  "description": "An Efficient Query Parameter Management Tool for React / Next.js. It helps you to manage query parameters in a more efficient way.",
  "repository": {
    "type": "git",
    "url": "git@github.com:abhishekbhardwaj/react-url-query-parameter-store.git"
  },
  "author": "Abhishek Bhardwaj <abhishek66ster@gmail.com>",
  "license": "MIT",
  "main": "./dist/cjs/src/index.js",
  "module": "./dist/esm/src/index.js",
  "types": "./dist/cjs/index.d.ts",
  "exports": {
    "types": "./dist/cjs/index.d.ts",
    "require": "./dist/cjs/src/index.js",
    "import": "./dist/esm/src/index.js",
    "default": "./dist/esm/src/index.js"
  },
  "files": [
    "/dist"
  ],
  "scripts": {
    "dev:commit": "cz",
    "dev:open-bundle-visualizer": "open rollup-plugin-visualizer-stats.html",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "build": "yarpm clean && rollup -c --bundleConfigAsCjs",
    "typecheck": "tsc --noEmit -p tsconfig.build.json",
    "publish-lib": "yarpm run build && yarpm publish",
    "publish-beta": "yarpm run build && yarpm publish --tag beta",
    "publish-dryrun": "yarpm run build && yarpm publish --dry-run",
    "clean": "rimraf dist rollup-plugin-visualizer-stats.html",
    "reset": "yarpm clean && git clean -xdf node_modules",
    "update-all-dependencies": "npx npm-check-updates -u",
    "lint": "eslint './{src,test}/**/*' --ext .js,.ts",
    "format": "eslint './{src,test}/**/*' --ext .js,.ts --fix",
    "prepare": "is-ci || husky"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@ianvs/prettier-plugin-sort-imports": "^4.2.1",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.7",
    "@rollup/plugin-terser": "^0.4.4",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@size-limit/preset-big-lib": "^11.1.4",
    "@testing-library/react": "^16.0.1",
    "@types/lodash": "^4.17.6",
    "@types/node": "^20.14.7",
    "@types/react": "^18.3.3",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.13.1",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "3.3.0",
    "devmoji": "^2.3.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-sonarjs": "^1.0.3",
    "eslint-plugin-unused-imports": "^3.2.0",
    "husky": "^9.0.11",
    "is-ci": "^3.0.1",
    "jsdom": "^25.0.1",
    "lint-staged": "^15.2.7",
    "next": "^14.2.4",
    "next-router-mock": "^0.9.13",
    "open": "^10.1.0",
    "prettier": "^3.3.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rimraf": "^5.0.7",
    "rollup": "^4.18.0",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-typescript2": "^0.36.0",
    "rollup-plugin-visualizer": "^5.12.0",
    "semantic-release": "^24.0.0",
    "size-limit": "^11.1.4",
    "typescript": "^5.5.2",
    "vite-tsconfig-paths": "^5.0.1",
    "vitest": "^2.1.1",
    "yarpm": "^1.2.0",
    "zod": "^3.23.8"
  },
  "peerDependencies": {
    "next": "^14.2.4",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "zod": "^3.23.8"
  },
  "size-limit": [
    {
      "path": "dist/*.js"
    }
  ],
  "dependencies": {
    "lodash": "^4.17.21",
    "tslib": "^2.6.3"
  },
  "packageManager": "pnpm@9.2.0"
}
